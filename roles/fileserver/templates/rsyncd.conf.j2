uid             = {{ user }}
gid             = {{ user }}
use chroot      = no
max connections = 8
log file        = /home/{{ user }}/log/rsync.log
pid file        = /home/{{ user }}/run/rsync.pid
lock file       = /home/{{ user }}/run/rsync.lock
timeout         = 600

[storage]
    path = /home/{{ user }}/storage
    comment = storage files
    read only = FALSE
